{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "endOfLine": "auto",
  "printWidth": 120,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^[./]", "^@/http/(.*)$", "^@/lib/(.*)$", "^@/components/(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
